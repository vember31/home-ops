{
  "Servers": {
    "1": {
      "Name": "postgres-v17",
      "Group": "Kubernetes",
      "Host": "postgres-17-rw.database.svc.cluster.local",
      "Port": 5432,
      "MaintenanceDB": "postgres",
      "Username": "{{ .POSTGRES_SUPER_USER }}",
      "Shared": true,
      "SharedUsername": "{{ .POSTGRES_SUPER_USER }}",
      "ConnectionParameters": {
          "sslmode": "prefer",
          "connect_timeout": 10,
          "passfile": "/.pgpass"
      },
      "Tags": [
        {
          "color": "#EC0BB4",
          "text": "Cluster"
        }
      ],
      "PostConnectionSQL": "set timezone='${TIMEZONE}'"
    }
  }
}